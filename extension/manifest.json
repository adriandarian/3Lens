{
  "name": "3Lens Extension",
  "description": "A sample TypeScript browser extension using 3Lens.",
  "version": "0.1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "dist/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/content.js"],
      "run_at": "document_idle"
    }
  ],
  "host_permissions": ["<all_urls>"]
}
