{
  "name": "@3lens/ui-core",
  "version": "1.0.0",
  "description": "3Lens UI core - framework-agnostic UI shell",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@3lens/kernel": "workspace:*",
    "@3lens/runtime": "workspace:*"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  },
  "keywords": ["3lens", "threejs", "devtools", "ui"],
  "license": "MIT"
}
