{
  "name": "@3lens/vue-bridge",
  "version": "0.1.0-alpha.1",
  "description": "Vue and TresJS integration for 3Lens - the three.js devtool",
  "keywords": [
    "three.js",
    "threejs",
    "vue",
    "tresjs",
    "devtools",
    "debugging"
  ],
  "license": "MIT",
  "author": "",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@3lens/core": "workspace:*"
  },
  "devDependencies": {
    "@tresjs/core": "^4.0.0",
    "@vue/test-utils": "^2.4.6",
    "jsdom": "^27.4.0",
    "three": "^0.160.0",
    "vue": "^3.4.0"
  },
  "peerDependencies": {
    "three": ">=0.150.0",
    "vue": ">=3.3.0"
  },
  "peerDependenciesMeta": {
    "@tresjs/core": {
      "optional": true
    }
  }
}
