{
  "name": "@3lens/vue-bridge",
  "version": "0.1.0-alpha.1",
  "description": "Vue and TresJS integration for 3Lens - the three.js devtool",
  "keywords": [
    "three.js",
    "threejs",
    "vue",
    "tresjs",
    "devtools",
    "debugging"
  ],
  "homepage": "https://github.com/adriandarian/3Lens",
  "repository": {
    "type": "git",
    "url": "https://github.com/adriandarian/3Lens",
    "directory": "packages/vue-bridge"
  },
  "license": "MIT",
  "author": "Adrian Darian <adrian.the.hactus@gmail.com> (https://github.com/adriandarian)",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
