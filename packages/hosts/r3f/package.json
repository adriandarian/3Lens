{
  "name": "@3lens/host-r3f",
  "version": "1.0.0",
  "description": "3Lens host for React Three Fiber applications",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@3lens/kernel": "workspace:*",
    "@3lens/runtime": "workspace:*"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "three": ">=0.150.0",
    "@react-three/fiber": ">=8.0.0",
    "react": ">=18.0.0"
  },
  "keywords": [
    "3lens",
    "threejs",
    "devtools",
    "host",
    "react-three-fiber",
    "r3f"
  ],
  "license": "MIT"
}
